{
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "lint": {
      "outputs": []
    },
    "clean": {
      "outputs": []
    },
    "dev": {
      "cache": false
    }
  },
  "globalDependencies": [
    "$NEXT_PUBLIC_API_URL",
    "$APP_ENV",
    "$NODE_ENV",
    "$PORT",
    "$PORT",
    "$APP_URL",
    "$ALLOWED_ORIGINS",
    "$AWS_ACCESS_KEY_ID",
    "$AWS_SECRET_ACCESS_KEY",
    "$SESSION_SECRET",
    "$MAGIC_LINK_SECRET"
  ]
}
